<!DOCTYPE html>
<html>

<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css" integrity="sha384-Zug+QiDoJOrZ5t4lssLdxGhVrurbmBWopoEl+M6BdEfwnCJZtKxi1KgxUyJq13dy" crossorigin="anonymous">
  <!-- <link rel="stylesheet" href="/public/startbootstrap-sb-admin-gh-pages/css/sb-admin.min.css"> -->
  <% include header.ejs %>
</head>

<body>
  <h2 align="center">temperature</h2>
  <table class="table">
  <thead>
    <tr>
      <th scope="col">sensID</th>
      <th scope="col">teamID</th>
      <th scope="col">val</th>
      <th scope="col">date</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i < data.length; i++) { %>
    <tr>
      <th scope="row"><%= data[i].sensID %></th>
      <td><%= data[i].teamID %></td>
      <td><%= data[i].val %></td>
      <td><%= data[i].date %></td>
    </tr>
    <% } %>
  </tbody>
</table>
</body>

</html>
